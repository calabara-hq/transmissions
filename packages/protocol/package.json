{
  "name": "transmissions-protocol",
  "version": "1.0.0",
  "author": "nick",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true,
  "scripts": {
    "test-gas": "forge test --gas-report",
    "prettier:check": "prettier --check 'src/**/*.sol' 'test/**/*.sol' 'package/**/*.ts' 'wagmi.config.ts'",
    "prettier": "prettier --write 'src/**/*.sol' 'test/**/*.sol' 'package/**/*.ts' 'script/**/*.ts' 'wagmi.config.ts'",
    "generate-abis": "yarn tsx script/generate-abis.ts",
    "build": "yarn wagmi && yarn generate-abis && yarn prettier && tsup",
    "wagmi": "forge build && wagmi generate"
  },
  "devDependencies": {
    "@types/node": "^20.12.10",
    "@wagmi/cli": "^2.1.4",
    "glob": "^10.3.12",
    "pathe": "^1.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "tsup": "^8.0.2",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5"
  }
}
